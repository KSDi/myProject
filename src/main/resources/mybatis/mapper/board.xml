<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="board">
	<select id="selectQuestionList" parameterType="String" resultType="Question">
		select * from question where users_id = #{value}
	</select>
</mapper>



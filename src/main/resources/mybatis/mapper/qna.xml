<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="qna">
	<select id="selectList" parameterType="String" resultType="Question">
		select * from question where users_id = #{value}
	</select>
	<insert id="insertQna" parameterType="Question">
		insert into question values(seq_qna_id.nextval, #{users_id}, #{title}, #{content},sysdate,#{status})
	</insert>
</mapper>


